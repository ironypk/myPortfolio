.contacts{
    position: relative;
}
.contacts__container{
    padding-top:50px;
    padding-bottom:140px;
    display: flex;
    flex-direction: column;
    justify-content: center;
    @include tablets{
        align-items: center;
    }
}
.contacts__title{
    font-size: 36px;
    font-weight: bold;
    margin-bottom: 80px;
    @include phones{
        font-size: 24px;
        margin-bottom:40px;
    }
}


